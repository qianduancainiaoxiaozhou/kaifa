.contaner5 {
  margin-top: 30rpx;
  background-color: #fff;
  padding-top: 32rpx;
  box-sizing: border-box;
  padding-bottom: 45rpx;
}
.contaner5 .top1 {
  width: 400rpx;
  margin: 0 auto;
  display: flex;
  justify-content: space-between;
  font-size: 20rpx;
  color: #686868;
  vertical-align: middle;
}
.contaner5 .top1 text {
  margin-left: 10rpx;
}
.content {
  width: 690rpx;
  margin: 20rpx auto 0rpx;
  font-size: 32rpx;
  color: #686868;
}
.title {
  display: flex;
  height: 60rpx;
  border-bottom: 2rpx solid #f1f1f1;
  justify-content: space-between;
}
.item {
  width: 100%;
  height: 74rpx;
  line-height: 74rpx;
  display: flex;
  justify-content: space-between;
  border-bottom: 2rpx solid #f1f1f1;
}
.item-font {
  width: 254rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

